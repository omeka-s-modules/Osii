@prefix : <http://omeka.org/s/vocabs/o-module-osii#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

:source_resource a rdf:Property ;
    rdfs:label "source resource"@en ;
    rdfs:comment "The URL to the source resource."@en ;
    rdfs:subPropertyOf dcterms:source .

:source_site a rdf:Property ;
    rdfs:label "source site"@en ;
    rdfs:comment "The URL to the site from which this resource was derived."@en ;
    rdfs:subPropertyOf dcterms:source .
